{
  "globalStyle": {
    "navigationStyle": "custom",
    "navigationBarTitleText": "小马养梯",
    "navigationBarBackgroundColor": "#f8f8f8",
    "navigationBarTextStyle": "black",
    "backgroundColor": "#FFFFFF"
  },
  "easycom": {
    "autoscan": true,
    "custom": {
      "^wd-(.*)": "wot-design-uni/components/wd-$1/wd-$1.vue",
      "^(?!z-paging-refresh|z-paging-load-more)z-paging(.*)": "z-paging/components/z-paging$1/z-paging$1.vue"
    }
  },
  "tabBar": {
    "color": "#8A8A8A",
    "selectedColor": "#539DF3",
    "backgroundColor": "#F8F8F8",
    "borderStyle": "black",
    "height": "50px",
    "fontSize": "10px",
    "iconWidth": "24px",
    "spacing": "3px",
    "list": [
      {
        "iconPath": "static/tabbar/home.png",
        "selectedIconPath": "static/tabbar/home-select.png",
        "pagePath": "pages/index/index",
        "text": "首页"
      },
      {
        "iconPath": "static/tabbar/maintenance.png",
        "selectedIconPath": "static/tabbar/maintenance-select.png",
        "pagePath": "pages/maintenance/maintenance",
        "text": "保养"
      },
      {
        "iconPath": "static/tabbar/forewarning.png",
        "selectedIconPath": "static/tabbar/forewarning-select.png",
        "pagePath": "pages/forewarning/forewarning",
        "text": "急修"
      },
      {
        "iconPath": "static/tabbar/personal.png",
        "selectedIconPath": "static/tabbar/personal-select.png",
        "pagePath": "pages/personal/personal",
        "text": "我的"
      }
    ]
  },
  "pages": [
    {
      "path": "pages/login/login",
      "type": "home",
      "layout": "default",
      "style": {
        "navigationBarTitleText": "登录"
      }
    },
    {
      "path": "pages/index/index",
      "type": "home",
      "style": {
        "navigationBarTitleText": "首页"
      }
    },
    {
      "path": "pages/forewarning/forewarning",
      "type": "page",
      "layout": "default",
      "style": {
        "navigationBarTitleText": "急修列表"
      }
    },
    {
      "path": "pages/maintenance/maintenance",
      "type": "page",
      "style": {
        "navigationBarTitleText": "保养"
      }
    },
    {
      "path": "pages/personal/personal",
      "type": "page",
      "style": {
        "navigationBarTitleText": "我的"
      }
    }
  ],
  "subPackages": [
    {
      "root": "pages-sub",
      "pages": [
        {
          "path": "shop/shop",
          "type": "page",
          "layout": "default",
          "style": {
            "navigationBarTitleText": "小马商城"
          }
        },
        {
          "path": "sign-in/sign-in",
          "type": "page",
          "layout": "default",
          "style": {
            "navigationBarTitleText": "签到打卡"
          }
        },
        {
          "path": "knowledge/category/category",
          "type": "page",
          "layout": "default",
          "style": {
            "navigationBarTitleText": "电梯知识库文件夹目录"
          }
        },
        {
          "path": "knowledge/company/company",
          "type": "page",
          "layout": "default",
          "style": {
            "navigationBarTitleText": "电梯知识库"
          }
        },
        {
          "path": "knowledge/file/file",
          "type": "page",
          "layout": "default",
          "style": {
            "navigationBarTitleText": "电梯知识库文件目录"
          }
        },
        {
          "path": "lift/detail/detail",
          "type": "page",
          "layout": "default",
          "style": {
            "navigationBarTitleText": "电梯详情"
          }
        },
        {
          "path": "lift/enter/enter",
          "type": "page",
          "layout": "default",
          "style": {
            "navigationBarTitleText": "电梯信息录入"
          }
        },
        {
          "path": "lift/list/list",
          "type": "page",
          "layout": "default",
          "style": {
            "navigationBarTitleText": "电梯列表"
          }
        },
        {
          "path": "lift/monitor/monitor",
          "type": "page",
          "layout": "default",
          "style": {
            "navigationBarTitleview": "电梯监控"
          }
        },
        {
          "path": "sign-in/jp-signature/jp-signature",
          "type": "page"
        }
      ]
    }
  ]
}